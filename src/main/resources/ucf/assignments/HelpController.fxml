<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.HelpController">
   <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true">
      <Image url="@../Images/BackgroundImage.jpg" />
   </ImageView>
   <Text fill="#b49b8a" layoutX="134.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" text="HELP: Adding Items">
      <font>
         <Font name="Tahoma Bold" size="33.0" />
      </font>
   </Text>
   <Text fill="#d0b49f" layoutX="276.0" layoutY="381.0" strokeType="OUTSIDE" strokeWidth="0.0" text="1 | 6">
      <font>
         <Font name="Tahoma Bold" size="19.0" />
      </font>
   </Text>
   <Button fx:id="nextButton" layoutX="522.0" layoutY="357.0" mnemonicParsing="false" onAction="#nextButtonPressed" style="-fx-background-color: #d0b49f;" text="NEXT" textFill="#fef2f2">
      <font>
         <Font name="Tahoma Bold" size="16.0" />
      </font>
   </Button>
   <ImageView fitHeight="154.0" fitWidth="262.0" layoutX="104.0" layoutY="73.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@../Images/addItem.png" />
   </ImageView>
   <Text fill="#b49b8a" layoutX="104.0" layoutY="274.0" strokeType="OUTSIDE" strokeWidth="0.0" text="To add an item to the inventory tracker:">
      <font>
         <Font name="Tahoma" size="22.0" />
      </font>
   </Text>
   <Text fill="#b49b8a" layoutX="181.0" layoutY="330.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Then, press &quot;ADD ITEM&quot;.">
      <font>
         <Font size="22.0" />
      </font>
   </Text>
   <Text fill="#b49b8a" layoutX="75.0" layoutY="302.0" strokeType="OUTSIDE" strokeWidth="0.0" text="fill in the item name, serial number, and price.">
      <font>
         <Font name="Tahoma" size="22.0" />
      </font>
   </Text>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
   <Text fill="#b49b8a" layoutX="389.0" layoutY="106.0" strokeType="OUTSIDE" strokeWidth="0.0" text="←">
      <font>
         <Font name="Tahoma Bold" size="58.0" />
      </font>
   </Text>
   <Text fill="#b49b8a" layoutX="389.0" layoutY="143.0" strokeType="OUTSIDE" strokeWidth="0.0" text="←">
      <font>
         <Font name="Tahoma Bold" size="58.0" />
      </font>
   </Text>
   <Text fill="#b49b8a" layoutX="389.0" layoutY="180.0" strokeType="OUTSIDE" strokeWidth="0.0" text="←">
      <font>
         <Font name="Tahoma Bold" size="58.0" />
      </font>
   </Text>
   <Text fill="#3e2a2a" layoutX="389.0" layoutY="209.0" strokeType="OUTSIDE" strokeWidth="0.0" text="←">
      <font>
         <Font name="Tahoma Bold" size="58.0" />
      </font>
   </Text>
   <Button fx:id="returnButton" layoutX="11.0" layoutY="14.0" mnemonicParsing="false" onAction="#returnButtonPressed" prefHeight="42.0" prefWidth="47.0" style="-fx-background-color: #d0b49f;" text="X" textFill="#fef2f2">
      <font>
         <Font name="Tahoma Bold" size="22.0" />
      </font>
   </Button>
</AnchorPane>
